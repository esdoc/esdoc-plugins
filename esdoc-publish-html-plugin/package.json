{
  "name": "esdoc-publish-html-plugin",
  "version": "2.0.0",
  "description": "A publish HTML plugin for ESDoc",
  "author": "h13i32maru",
  "homepage": "https://github.com/esdoc/esdoc-plugins",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/esdoc/esdoc-plugins"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.3",
    "fs-extra": "7.0.1",
    "escape-html": "1.0.3",
    "@babel/generator": "^7.3.2",
    "ice-cap": "0.0.4",
    "marked": "^0.6.2",
    "taffydb": "2.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/register": "^7.0.0",
    "esdoc": "sebastianwessel/esdoc",
    "esdoc-accessor-plugin": "sebastianwessel/esdoc-plugins",
    "esdoc-brand-plugin": "sebastianwessel/esdoc-plugins",
    "esdoc-coverage-plugin": "sebastianwessel/esdoc-plugins",
    "esdoc-ecmascript-proposal-plugin": "sebastianwessel/esdoc-plugins",
    "esdoc-external-ecmascript-plugin": "sebastianwessel/esdoc-plugins",
    "esdoc-integrate-manual-plugin": "sebastianwessel/esdoc-plugins",
    "esdoc-integrate-test-plugin": "sebastianwessel/esdoc-plugins",
    "esdoc-type-inference-plugin": "sebastianwessel/esdoc-plugins",
    "esdoc-undocumented-identifier-plugin": "sebastianwessel/esdoc-plugins",
    "esdoc-unexported-identifier-plugin": "sebastianwessel/esdoc-plugins",
    "mocha": "6.1.4"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "build": "rm -rf ./out/src && babel --out-dir out/src --ignore 'Builder/template' src && cp -a src/Builder/template out/src/Builder/",
    "test": "rm -rf ./test/fixture/out && mocha -t 10000 --require ./node_modules/babel-register --require ./test/src/init.js --recursive ./test/src -R spec"
  },
  "keywords": [
    "esdoc",
    "publish",
    "html",
    "plugin"
  ],
  "files": [
    "out/src",
    "README.md"
  ],
  "main": "out/src/Plugin.js"
}
